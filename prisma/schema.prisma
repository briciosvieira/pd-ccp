generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model pdcompras {
  id    Int    @id @default(autoincrement())
  num_pd_venda String @unique
  num_quote String @unique
  num_proposta String @unique
}